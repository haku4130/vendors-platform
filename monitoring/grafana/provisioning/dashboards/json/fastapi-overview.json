{
  "title": "FastAPI Overview",
  "uid": "fastapi-overview",
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 0,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate",
      "type": "graph",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"backend\"}[5m])",
          "legendFormat": "{{method}} {{handler}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        { "format": "reqps", "label": "Requests/sec" },
        { "format": "short" }
      ]
    },
    {
      "id": 2,
      "title": "Response Time (p95)",
      "type": "graph",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"backend\"}[5m]))",
          "legendFormat": "{{method}} {{handler}} p95",
          "refId": "A"
        }
      ],
      "yaxes": [{ "format": "s", "label": "Duration" }, { "format": "short" }]
    },
    {
      "id": 3,
      "title": "Error Rate",
      "type": "graph",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"backend\",status=~\"5..\"}[5m])",
          "legendFormat": "{{method}} {{handler}} - {{status}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        { "format": "reqps", "label": "Errors/sec" },
        { "format": "short" }
      ]
    },
    {
      "id": 4,
      "title": "Active Requests",
      "type": "graph",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "http_requests_inprogress{job=\"backend\"}",
          "legendFormat": "In Progress",
          "refId": "A"
        }
      ],
      "yaxes": [
        { "format": "short", "label": "Requests" },
        { "format": "short" }
      ]
    }
  ]
}
